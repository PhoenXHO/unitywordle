                    <ui:VisualElement name="WordContainer" style="flex-grow: 0; background-color: rgb(43, 75, 87); width: auto; height: auto; justify-content: center; align-items: center; border-left-color: rgb(167, 210, 248); border-right-color: rgb(167, 210, 248); border-top-color: rgb(167, 210, 248); border-bottom-color: rgb(167, 210, 248); border-left-width: 2px; border-right-width: 2px; border-top-width: 2px; border-bottom-width: 2px; padding-left: 0; padding-right: 0; padding-top: 0; padding-bottom: 0; margin-left: 0; margin-right: 0; margin-top: 30px; margin-bottom: 20px; flex-shrink: 0;">
