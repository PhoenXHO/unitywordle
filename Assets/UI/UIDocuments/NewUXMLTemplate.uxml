    <ui:Button display-tooltip-when-elided="true" focusable="false" name="RestartButton" class="restart-button" style="position: relative; background-image: none; -unity-background-scale-mode: scale-to-fit; border-left-width: 0; border-right-width: 0; border-top-width: 0; border-bottom-width: 0; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-left: 26px; padding-right: 26px; padding-top: 26px; padding-bottom: 26px; width: 100px; height: 100px; transition-property: scale; transition-duration: 0.1s; border-left-color: rgb(102, 140, 164); border-right-color: rgb(102, 140, 164); border-top-color: rgb(102, 140, 164); border-bottom-color: rgb(102, 140, 164); background-color: rgb(64, 66, 66);">
