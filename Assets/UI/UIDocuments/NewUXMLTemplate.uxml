                    </ui:VisualElement>
                </ui:VisualElement>
                <ui:Label tabindex="-1" text="Congratulations! You got it right!" display-tooltip-when-elided="true" name="Congrats" style="color: rgb(255, 255, 255); font-size: 24px; position: relative; top: auto; -unity-font: url(&apos;project://database/Assets/TextMesh%20Pro/Fonts/LiberationSans.ttf?fileID=12800000&amp;guid=e3265ab4bf004d28a9537516768c1c75&amp;type=3#LiberationSans&apos;); margin-bottom: 0; width: 100%; align-self: center; -unity-text-align: upper-center; margin-left: auto; margin-right: auto; margin-top: 0; display: none;" />
                <ui:Button text="New Game" display-tooltip-when-elided="true" name="NewGameButton" focusable="false" class="new-game-button" style="margin-bottom: 0; border-left-color: rgb(84, 233, 82); border-right-color: rgb(84, 233, 82); border-top-color: rgb(84, 233, 82); border-bottom-color: rgb(84, 233, 82); transition-property: scale, background-color; transition-duration: 0.05s, 0s; transition-timing-function: ease, ease; transition-delay: 0s, 0.05s;" />
                <ui:Label tabindex="-1" text="or press ENTER to play again" display-tooltip-when-elided="true" style="color: rgba(255, 255, 255, 0.62); align-self: center; -unity-font: url(&apos;project://database/Assets/TextMesh%20Pro/Fonts/LiberationSans.ttf?fileID=12800000&amp;guid=e3265ab4bf004d28a9537516768c1c75&amp;type=3#LiberationSans&apos;); font-size: 14px;" />
