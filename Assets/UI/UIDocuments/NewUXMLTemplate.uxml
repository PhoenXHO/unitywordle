    <ui:VisualElement name="Menu" style="width: auto; align-items: center; justify-content: center; align-self: center; margin-left: auto; margin-right: auto; margin-top: auto; margin-bottom: auto; transition-property: scale, opacity; transition-duration: 0.2s, 0.2s;">
